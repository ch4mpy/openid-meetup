{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": ".",
  "projects": {
    "cafe-skifo": {
      "projectType": "application",
      "schematics": {},
      "root": "cafe-skifo",
      "sourceRoot": "cafe-skifo/src",
      "prefix": "bar",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/cafe-skifo",
            "index": "cafe-skifo/src/index.html",
            "main": "cafe-skifo/src/main.ts",
            "polyfills": "cafe-skifo/src/polyfills.ts",
            "tsConfig": "cafe-skifo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "cafe-skifo/src/silent-renew.html",
              "cafe-skifo/src/favicon.ico",
              "cafe-skifo/src/assets",
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "cafe-skifo/src/styles.css",
              {
                "input": "node_modules/@ionic/angular/css/normalize.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/structure.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/typography.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/core.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/padding.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/float-elements.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/text-alignment.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/text-transformation.css"
              },
              {
                "input": "node_modules/@ionic/angular/css/flex-utils.css"
              },
              {
                "input": "cafe-skifo/src/theme/variables.css"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "laptop-jerem": {
              "fileReplacements": [
                {
                  "replace": "cafe-skifo/src/environments/environment.ts",
                  "with": "cafe-skifo/src/environments/environment.laptop-jerem.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "cafe-skifo/src/environments/environment.ts",
                  "with": "cafe-skifo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cafe-skifo:build",
            "host": "laptop-jerem",
            "ssl": true,
            "sslCert": "./laptop-jerem_self_signed.crt",
            "sslKey": "./laptop-jerem_self_signed_key.pem"
          },
          "configurations": {
            "laptop-jerem": {
              "browserTarget": "cafe-skifo:build:laptop-jerem"
            },
            "production": {
              "browserTarget": "cafe-skifo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cafe-skifo:build"
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "cafe-skifo:ionic-cordova-build",
            "devServerTarget": "cafe-skifo:serve"
          },
          "configurations": {
            "laptop-jerem": {
              "cordovaBuildTarget": "cafe-skifo:ionic-cordova-build:laptop-jerem",
              "devServerTarget": "cafe-skifo:serve:laptop-jerem"
            },
            "production": {
              "cordovaBuildTarget": "cafe-skifo:ionic-cordova-build:production",
              "devServerTarget": "cafe-skifo:serve:production"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "cafe-skifo:build"
          },
          "configurations": {
            "laptop-jerem": {
              "browserTarget": "cafe-skifo:build:laptop-jerem"
            },
            "production": {
              "browserTarget": "cafe-skifo:build:production"
            }
          }
        }
      }
    },
    "@tahiti-devops/bar-api": {
      "projectType": "library",
      "root": "libs/tahiti-devops/bar-api",
      "sourceRoot": "libs/tahiti-devops/bar-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "libs/tahiti-devops/bar-api/tsconfig.lib.json",
            "project": "libs/tahiti-devops/bar-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tahiti-devops/bar-api/tsconfig.lib.prod.json"
            }
          }
        }
      }
    }
  },
  "defaultProject": "cafe-skifo",
  "cli": {
    "defaultCollection": "@ionic/angular-toolkit",
    "analytics": false
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}
