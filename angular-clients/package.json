{
  "name": "angular-clients",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "cd libs/tahiti-devops/bar-api && npm i && cd ../../.. && npm run bar-client:build && cd ionic-bar && npm i",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "bar-client:generate": "node node_modules/@openapitools/openapi-generator-cli/bin/openapi-generator generate -g typescript-angular --type-mappings AnyType=any --additional-properties=serviceSuffix=RestClient,npmName=@tahiti-devops/bar-api,stringEnums=true,enumPropertyNaming=camelCase --remove-operation-id-prefix -i ../bar.openapi.json -o libs/tahiti-devops/bar-api",
    "bar-client:build": "npm run bar-client:generate && npm run ng -- build @tahiti-devops/bar-api --prod",
    "ionic-bar:ch4mpy": "ionic cap run android --project=ionic-bar --host=192.168.8.100 --source-map --configuration=ch4mpy -l"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@ionic/angular": "^5.3.2",
    "@ionic/pwa-elements": "^3.0.1",
    "@ionic/storage": "^2.3.1",
    "angular-auth-oidc-client": "^11.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@ionic/angular-toolkit": "latest",
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
