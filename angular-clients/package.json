{
  "name": "angular-clients",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "cd libs/tahiti-devops/cafe-skifo && npm i && cd ../../.. && npm run cafe-skifo-client:build && cd cafe-skifo && npm i",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "cafe-skifo-client:generate": "node node_modules/@openapitools/openapi-generator-cli/bin/openapi-generator generate -g typescript-angular --type-mappings AnyType=any --additional-properties=serviceSuffix=RestClient,npmName=@tahiti-devops/cafe-skifo,stringEnums=true,enumPropertyNaming=camelCase --remove-operation-id-prefix -i ../cafe-skifo.openapi.json -o libs/tahiti-devops/cafe-skifo",
    "cafe-skifo-client:build": "npm run cafe-skifo-client:generate && npm run ng -- build @tahiti-devops/cafe-skifo --prod",
    "cafe-skifo:serve": "ionic serve --ssl --external --public-host=ch4mpy-bravo --configuration=ch4mpy-bravo",
    "cafe-skifo:android": "ionic capacitor run android -l --ssl --external --public-host=ch4mpy-bravo --configuration=ch4mpy-bravo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@capacitor/android": "^2.4.0",
    "@capacitor/core": "2.4.0",
    "@capacitor/ios": "^2.4.2",
    "@ionic-native/splash-screen": "^5.35.0",
    "@ionic-native/status-bar": "^5.35.0",
    "@ionic-native/core": "^5.34.0",
    "@ionic-native/deeplinks": "^5.34.0",
    "@ionic/angular": "^5.6.13",
    "@ionic/storage-angular": "^3.0.6",
    "ionic-plugin-deeplinks": "^1.0.20",
    "angular-auth-oidc-client": "^11.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.1.7",
    "@angular/compiler-cli": "~10.1.0",
    "@ionic/angular": "^5.6.12",
    "@ionic/angular-toolkit": "^4.0.0",
    "@ionic/cli": "^6.16.3",
    "@ionic/lab": "^3.2.10",
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
